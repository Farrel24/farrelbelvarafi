    private void combo_jenis1ActionPerformed(java.awt.event.ActionEvent evt) {                                             
        if(combo_jenis1.getSelectedItem().equals("25.000")){
            text_harga.setText("27000");
        }else if(combo_jenis1.getSelectedItem().equals("50.000")){
            text_harga.setText("52000");
        }else if(combo_jenis1.getSelectedItem().equals("75.000")){
            text_harga.setText("77000");
        }else if(combo_jenis1.getSelectedItem().equals("100.000")){
            text_harga.setText("102000");

        }
    }                                            

    private void btn_bayarActionPerformed(java.awt.event.ActionEvent evt) {                                          
       int bayar = Integer.parseInt(text_bayar.getText());
        int totalharga = Integer.parseInt(text_harga.getText());
        int kembalian = bayar-totalharga;
        text_kembalian.setText(String.valueOf(kembalian));
        
    
           
            
    }                                         

    private void text_bayarActionPerformed(java.awt.event.ActionEvent evt) {                                           
