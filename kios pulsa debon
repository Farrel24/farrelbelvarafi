    }                                          

    private void text_kembalianKeyReleased(java.awt.event.KeyEvent evt) {                                           
        
    }                                          

    private void btn_cancelActionPerformed(java.awt.event.ActionEvent evt) {                                           
        cancel();
    }                                          

